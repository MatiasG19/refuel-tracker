{
  "name": "refuel-tracker",
  "version": "0.0.1",
  "description": "Tracks your refuels.",
  "productName": "Refuel Tracker",
  "author": "MatiasG19",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "check-format": "yarn prettier --check ./src",
    "test": "vitest run",
    "dev": "quasar dev",
    "build": "quasar build",
    "mode": "quasar mode add capacitor",
    "appdev": "quasar dev -m capacitor -T android",
    "appbuild": "quasar build -m capacitor -T android",
    "favicons": "icongenie generate --theme-color 8488ff --icon ./.icons/icon.png",
    "appicons": "icongenie generate --mode capacitor --theme-color 8488ff --skip-trim --icon ./.icons/appicon.png",
    "splashicons": "icongenie generate --mode capacitor --filter splashscreen --splashscreen-icon-ratio 30 --theme-color 8488ff --skip-trim --icon ./.icons/icon.png"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.6",
    "axios": "^1.2.1",
    "dexie": "^3.2.4",
    "dexie-export-import": "^4.0.7",
    "mitt": "^3.0.1",
    "pinia": "^2.0.11",
    "quasar": "^2.12.6",
    "vue": "^3.0.0",
    "vue-i18n": "^9.4.1",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@capacitor/core": "^5.3.0",
    "@capacitor/filesystem": "^5.1.3",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/local-notifications": "^5.0.6",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.5.1",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vitest": "^0.34.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
